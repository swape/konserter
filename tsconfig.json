{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"outDir": "./dist",
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"target": "ESNext",
		"noEmit": true,
	},
	"exclude": ["node_modules"]
}
