{
  "name": "konserter",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --fix .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.42",
    "@typescript-eslint/parser": "^5.42",
    "autoprefixer": "^10.4",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-simple-import-sort": "^8",
    "eslint-plugin-svelte3": "^4",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8",
    "svelte": "^3",
    "svelte-check": "^2",
    "svelte-preprocess": "^4",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.8.4"
  },
  "type": "module",
  "dependencies": {
    "cookie": "^0.5.0",
    "firebase": "^9.13.0"
  }
